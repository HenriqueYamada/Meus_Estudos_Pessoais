algoritmo "semnome"
var
   cont: INTEIRO
inicio
      cont <- 30
      Enquanto (cont >= 1) faca
               Se (cont % 4 = 0) entao
                  Escreva(" [", cont, "] ")
               Senao
                    Escreva(" ", cont, " ")
               FimSe
               cont <- cont - 1
      FimEnquanto
fimalgoritmo
