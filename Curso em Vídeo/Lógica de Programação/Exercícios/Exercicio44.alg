algoritmo "semnome"
var
   inic, fim, incr: INTEIRO
inicio
      Escreva("Digite o primeiro Valor: ")
      Leia(inic)
      Escreva("Digite o último Valor: ")
      Leia(fim)
      Escreva("Digite o incremento: ")
      Leia(incr)
      Enquanto (inic <= fim) faca
               Escreva(inic ," ")
               inic <- inic + incr
      FimEnquanto
      Escreva("Acabou!")
fimalgoritmo
