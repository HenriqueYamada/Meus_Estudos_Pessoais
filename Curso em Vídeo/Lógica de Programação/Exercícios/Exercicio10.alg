algoritmo "Exercício09"
var
   largura, altura, area, tinta: REAL
inicio
      Escreva("Largura: ")
      Leia (largura)
      Escreva("Altura: ")
      Leia(altura)
      area <- largura * altura
      tinta <- area / 2
      EscrevaL("Área da parede: ", area)
      Escreva("Tinta a ser usada: ", tinta)
fimalgoritmo
